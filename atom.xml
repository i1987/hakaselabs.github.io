---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>{{ site.title | xml_escape }}</title>
  <link href="http://hakaselabs.github.io/"/>
  <link type="application/atom+xml" rel="self" href="http://hakaselabs.io/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://example.com/</id>
  <author>
    <name>{{ site.author.name }}</name>
    <email>{{ site.author.email }}</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://hakaselabs.github.io{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://hakaselabs.github.io{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{ site.author.name }}</name>
      <email>{{ site.author.email }}</email>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
